<%- include("partials/header"); -%>
<h1><%= pageTitle %></h1>
<p><%= homeParagraph %></p>
<% composedPost.forEach(function(post){ %>
    <h2><%= post.title %></h2> 
    <p><%= post.postContent.substring(0,100)+" ..." %>
    <a href="/posts/<%= post.title %>">Read More</a></p>
<% }); %>
<%- include("partials/footer"); -%>